<div class="search">
  <form action="/search">
    <input name="query" value="{{keywords}}">
    <button>Search</button>
  </form>
</div>
{{#if error}}
  <p class="error">
    {{error}}
  </p>
{{else}}
  <div class="bookmarks">
    {{#each bookmarks }}
      {{> show_bookmark bookmark=this }}
    {{/each}}
  </div>
{{/if}}
